{% set _sc = this.stimulusController %}
<div{{ attributes }}
        {{ stimulus_controller(_sc) }}
>
    <button
            {{ stimulus_target(_sc, 'install') }}
            {{ stimulus_action(_sc, 'install') }}
    >
        <h1>PWA INSTALL????</h1>
        {% block install %}
        default install!
        {% endblock %}
    </button>

    <div
            {{ stimulus_target(_sc, 'launch') }}
            {{ stimulus_action(_sc, 'launch') }}
            >
        {{ block('launch') }}
    </div>

</div>
